<h2>配送先編集</h2>

<%= form_with(model: @address, url:address_path, local: true ,method: :patch) do |f| %>
  <div>
    <%= f.label :"郵便番号" %>
    <%= f.text_field :zipcode %>
  </div>
  <div>
    <%= f.label :"住所" %>
    <%= f.text_field :address %>
  </div>
  <div>
      <%= f.label :"宛名" %>
      <%= f.text_field :name %>
  </div>
  <div>
      <%= f.submit '編集する' %>
  </div>
<% end %>
